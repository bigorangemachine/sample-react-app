{
  "name": "movieapp",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "http-server": "node http-server.js > http-server.log",
    "postinstall": "mkdir -p ./_cache/ && chmod 777 ./_cache/ && npm run build && echo \"Setup Private data in '.env': refer to 'sample.env'.\"",
    "coverage": "NODE_ENV=test nyc npm run test",
    "test": "NODE_ENV=test mocha-webpack -R spec --require babel-core/register --webpack-config webpack.config.js \"pkg/**/*.test.jsx\" --require jsdom-global/register",
    "test:debug": "NODE_ENV=test-debug node --max_old_space_size=4096 --inspect --debug-brk ./node_modules/mocha-webpack/lib/cli -R spec --require babel-core/register --webpack-config webpack.config.js \"pkg/**/*.test.jsx\" --require jsdom-global/register",
    "test:watch": "npm run http-server | npm run test -- --watch",
    "build": "webpack --config webpack.config.js -d",
    "watch": "npm run http-server | npm run build -- --watch"
  },
  "keywords": [
    "movie"
  ],
  "author": "Brian Dinga",
  "license": "GPL-3.0",
  "dependencies": {
    "GLaDioS": "git://github.com/bigorangemachine/BOM-GLaDioS.js.git#v0.0.3",
    "axios": "^0.16.2",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "bom-utils": "git://github.com/bigorangemachine/BOM-nodejs-utils.git#v0.0.4",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "material-icons": "^0.1.0",
    "minireset.css": "0.0.2",
    "moment": "^2.18.1",
    "node-default-server": "git://github.com/bigorangemachine/BOM-default-server.git#v0.0.5",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.4.1",
    "react-router-dom": "^4.2.2",
    "react-sidenav": "^2.1.2",
    "sass-loader": "^4.1.0",
    "startbootstrap-simple-sidebar": "^4.0.0-alpha",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "webpack": "^3.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.1",
    "file-loader": "^0.9.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "merge": "^1.2.0",
    "mocha": "^3.5.0",
    "mocha-loader": "^1.1.1",
    "mocha-webpack": "^0.7.0",
    "nock": "^9.0.14",
    "node-sass": "^4.0.0",
    "nyc": "^11.1.0",
    "react-addons-create-fragment": "^15.4.1",
    "react-addons-test-utils": "^15.0.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^3.2.0",
    "source-map-support": "^0.4.15",
    "supertest": "^3.0.0",
    "webpack": "^3.5.3",
    "webpack-node-externals": "^1.5.4"
  },
  "nyc": {
    "require": [
      "babel-core/register"
    ],
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ],
    "exclude": [
      "webpack.*.js",
      "pkg/**/*.test.js",
      "pkg/**/*.test.jsx",
      ".tmp",
      "node_modules",
      "test-coverage",
      "**.css",
      "**.scss",
      "**.sass"
    ],
    "sourceMap": false,
    "instrument": false,
    "report-dir": "./test-coverage"
  }
}
